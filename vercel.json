{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    },
    {
        "$schema": "https://gemini.vercel.sh/vercel.json",
        "source": "/feedback/((?!general).*)",
        "destination": "/api/feedback/general"
    }
  ]
}