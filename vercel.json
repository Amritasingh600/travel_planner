{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env":{
      "$schema": "https://openapi.vercel.sh/vercel.json",
      "source": "/feedback/((?!general).*)",
      "destination": "/api/feedback/general"
  }
}